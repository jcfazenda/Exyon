 

{
  "name": "Lemon Captcha",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Google Chrome Extension that don't leaves Grooveshark stop playing.",
  
  "icons" : {
    "16" : "favicon.ico",
    "48" : "favicon.ico",
    "128" : "favicon.ico"
  },
  "content_scripts": [{
    "matches": 		["*://*/*"],
    "js": 			[ "index.js" ],
	"all_frames": 	true
  }]
}


